CREATE DATABASE drupal;
create database rudierdb;
CREATE USER rudieruser@localhost IDENTIFIED BY 'rvg';
GRANT SELECT,INSERT,UPDATE,DELETE,CREATE,DROP,INDEX,ALTER,CREATE TEMPORARY TABLES,LOCK TABLES ON drupal.* TO drupaluser@localhost;
GRANT SELECT,INSERT,UPDATE,DELETE,CREATE,DROP,INDEX,ALTER,CREATE TEMPORARY TABLES,LOCK TABLES ON rudierdb.* TO rudieruser@localhost;
FLUSH PRIVILEGES;
show databases;
